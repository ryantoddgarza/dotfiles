[alias]
	a = add
	b = branch
	cl = clone
	cm = commit
	co = checkout
	com = checkout master
	d = diff
	l = log
	s = status -s
	# `echo` until potentially destructive cases are tested. `git branch -d` after.
	dm = ! "git branch --merged | grep -ivE '\\*|master|develop' | xargs -n 1 echo"
	info = ! "inf() { if git rev-parse ${1} >/dev/null 2>&1; then git cat-file -p $(git rev-parse ${1}); else echo Invalid object; fi }; inf"
[color]
	ui = true
[core]
	excludesfile = ~/.gitignore
[credential]
	helper = osxkeychain
[http]
	postBuffer = 524288000
