[alias]
	a = add
	b = branch
	cl = clone
	cm = commit
	co = checkout
	d = diff
	l = log --oneline
	s = status --short
	info = ! "info() { if git rev-parse ${1} &>/dev/null; then git cat-file -p $(git rev-parse ${1}); else echo 'Invalid object'; fi }; info"
[color]
	ui = true
[core]
	excludesfile = ~/.gitignore
	ignorecase = false
[credential]
	helper = osxkeychain
[http]
	postBuffer = 524288000
