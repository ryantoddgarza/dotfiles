[alias]
  a = add
  b = branch
  c = commit
  co = checkout
  com = checkout master
  d = diff
  l = log
  s = status -s

	# Remove branches that have already been merged with main.
	dm = "!git branch --merged | grep -v '\\*' | xargs -n 1 git branch -d"
[color]
  ui = true
[core]
	excludesfile = ~/.gitignore
[credential]
	helper = osxkeychain
[http]
	postBuffer = 524288000
